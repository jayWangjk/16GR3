<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.qhit.lh.grs.wjk.t1.bean.User" table="t_user">
		<id name="uid" column="uid" type="integer">
			<generator class="native">
			</generator>
		</id>
		
		<property name="uname" column="uname" type="string"></property>
		<property name="password" column="password" type="string"></property>
		<property name="sex" column="sex" type="string"></property>
		<property name="education" column="education" type="string"></property>
	</class>
</hibernate-mapping>
